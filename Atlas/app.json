{
  "expo": {
    "name": "Atlas Workout Tracker",
    "slug": "atlas-workout-tracker",
    "description": "Track your workouts, build custom templates, and share progress with friends. Atlas makes fitness tracking simple and social.",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/atlas-logo.png",
    "scheme": "atlas",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "privacy": "public",
    "platforms": ["ios", "android"],
    "githubUrl": "https://github.com/user/atlas-workout-tracker",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.atlas.workouttracker",
      "buildNumber": "1",
      "infoPlist": {
        "CFBundleDisplayName": "Atlas"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/atlas-logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.atlas.workouttracker",
      "versionCode": 1,
      "edgeToEdgeEnabled": true,
      "permissions": []
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/atlas-logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/atlas-logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "your-eas-project-id-here"
      }
    }
  }
}
